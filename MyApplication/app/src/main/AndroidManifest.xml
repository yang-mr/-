<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yw.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TestIntentActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.jack.test" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="ee" />
                <data android:scheme="content" android:host="www.baidu.com"
                    android:path="/test/intent" android:mimeType="image/png" />
            </intent-filter>
            <intent-filter>
                <action android:name="com.jack.test1" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="ee" />
                <data android:scheme="content" android:host="www.baidu.com"
                    android:path="/test/intent" android:mimeType="image/png" />
            </intent-filter>
        </activity>
    </application>

</manifest>